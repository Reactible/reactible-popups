{
  "name": "reactible-popups",
  "version": "0.1.2",
  "description": "Reactible-popups allows you to create and customize web popups faster with lots of options.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "dev": "tsup src/index.tsx --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reactible/react-popups.git"
  },
  "keywords": [
    "react",
    "typescript",
    "popups"
  ],
  "author": "Rachid Lajmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Reactible/react-popups/issues"
  },
  "homepage": "https://github.com/Reactible/react-popups",
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
